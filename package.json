{
  "name": "@indot/usefetch",
  "version": "0.0.7",
  "description": "Custom fetch react hook",
  "main": "dist/useFetch.js",
  "types": "dist/useFetch.d.ts",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc --outDir ./dist",
    "gen-docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src; touch docs/.nojekyll"
  },
  "keywords": [
    "React",
    "Fetch",
    "Hook"
  ],
  "author": "jasmith79",
  "license": "MIT",
  "peerDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^2.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "fetch-ponyfill": "^6.1.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.9.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}
